<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjectKlientApplication.StartPage"
             Title="Профиль">

    <ContentPage.Content>
        <StackLayout Background="White" Padding="40,0,40,0">
            <Label Text = "Login" FontSize="Large" />
            <Entry x:Name = "LoginEntry" HorizontalTextAlignment="Center"></Entry>
            <Label Text = "Password" FontSize = "Large" />
            <Entry x:Name = "PasswordEntry" Placeholder = "Password" IsPassword ="True" HorizontalTextAlignment="Center"></Entry>
            <Button Text="In" Clicked="EnterButtonClicked" ></Button>
            <Button Text="Registration" Clicked="RegistrationButtonClicked" ></Button>
            <Button Text="Admin" Clicked="EnterAdminPanelButtonClicked" ></Button>
        </StackLayout>
    </ContentPage.Content>

</ContentPage>
